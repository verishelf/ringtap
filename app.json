{
  "expo": {
    "name": "RingTap",
    "slug": "ringtap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ringtap",
    "userInterfaceStyle": "automatic",
    "deeplinks": ["ringtap://activate", "ringtap://profile"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.ringtap.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "RingTap uses your photo library to set your profile picture and add a video intro.",
        "NSMicrophoneUsageDescription": "RingTap may use the microphone when recording a video intro.",
        "NSCameraUsageDescription": "RingTap may use the camera to take a profile photo or record a video intro.",
        "UIBackgroundModes": [],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "supabase.co": {
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionRequiresForwardSecrecy": false,
              "NSRequiresCertificateTransparency": false
            }
          }
        }
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "me.ringtap.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0B",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0B",
          "dark": {
            "backgroundColor": "#0A0A0B"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "RingTap uses your photo library to set your profile picture and add a video intro.",
          "cameraPermission": "RingTap uses the camera to take a profile photo or record a video intro.",
          "microphonePermission": "RingTap uses the microphone when recording a video intro."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63c87f6c-9296-48ee-ad7b-373c05cd4db1"
      },
      "supabaseUrl": "https://wdffdlznfthxwjhumacs.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6IndkZmZkbHpuZnRoeHdqaHVtYWNzIiwicm9sZSI6ImFub24iLCJpYXQiOjE3Njk4MTA5OTcsImV4cCI6MjA4NTM4Njk5N30.F9e_2swJPefLU_VLZw8LX05hwbqlhSzs-qaEWOT3YDA"
    }
  }
}
